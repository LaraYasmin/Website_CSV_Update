{
    "name": "shawandpartners",
    "version": "1.0.0",
    "description": "Fullstack-Javascript-Test",
    "main": "index.tsx",
    "scripts": {
      "test": "npm run test",
      "install:": "cd backend && npm install && npm run dev e cd frontend && npm install && npm run dev",
      "build:backend": "nodemon --exec node --loader ts-node/esm backend/src/index.ts",
      "build:frontend": "webpack --config frontend/webpack.config.js",
      "build": "concurrently \"npm run build:backend\" \"npm run build:frontend\"",
      "dev:backend": "nodemon --exec \"node --loader ts-node/esm\" backend/src/index.ts",
      "dev:frontend": "webpack serve --config frontend/webpack.config.js --mode development",
      "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\""
    },
    "author": "Lara Oliveira",
    "license": "ISC",
    "dependencies": {
      "express": "^4.19.2"
    },
    "devDependencies": {
      "@types/express": "^4.17.21",
      "@types/node": "^20.14.0",
      "concurrently": "^7.0.0",
      "svg-url-loader": "^8.0.0",
      "ts-loader": "^9.5.1",
      "ts-node-dev": "^2.0.0",
      "typescript": "4.9.5",
      "webpack-cli": "^5.1.4",
      "webpack-dev-server": "^5.0.4"
    }
  }  